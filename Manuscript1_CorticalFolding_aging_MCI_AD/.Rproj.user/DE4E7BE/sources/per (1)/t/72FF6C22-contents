  `SmoothArea_ROI_3_R`,
  `SmoothArea_ROI_4_R`,
  `SmoothArea_ROI_5_R`,
  `SmoothArea_ROI_6_R`
) %>%
  gather(
    `SmoothArea_ROI_1_L`,
    `SmoothArea_ROI_2_L`,
    `SmoothArea_ROI_3_L`,
    `SmoothArea_ROI_4_L`,
    `SmoothArea_ROI_5_L`,
    `SmoothArea_ROI_6_L`,
    `SmoothArea_ROI_1_R`,
    `SmoothArea_ROI_2_R`,
    `SmoothArea_ROI_3_R`,
    `SmoothArea_ROI_4_R`,
    `SmoothArea_ROI_5_R`,
