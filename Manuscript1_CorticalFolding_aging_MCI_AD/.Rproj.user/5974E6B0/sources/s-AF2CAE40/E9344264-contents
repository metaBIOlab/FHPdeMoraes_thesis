# DADOS CLINICOS

library(readxl)
clinic_data <-
  read_excel("~/Documentos/Gyrification/data/SUBJ FERNANDA H.xlsx", sheet = "Planilha1", na = "NA")

SUBJ_ESC_COGNITIVE <- read_excel("~/Documentos/Gyrification/data/SUBJ-ESC-COGNITIVE.xlsx")

#View(clinic_data)

colnames(clinic_data)[which(names(clinic_data) == "SUBJ_clean")] <-
  "SUBJ"

clinic_data <- mutate(clinic_data, Session = 1)

clinic_data$relogio <- as.integer(clinic_data$relogio)

clinic_data$Session <- as.integer(clinic_data$Session)
dados$Session <- as.integer(dados$Session)

dados <- left_join(dados, clinic_data)

dados <- left_join(dados, SUBJ_ESC_COGNITIVE)
