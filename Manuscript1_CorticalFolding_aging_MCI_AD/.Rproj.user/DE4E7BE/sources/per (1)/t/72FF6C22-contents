# K_ds
LobesExtract_ALL_K_ds <- LobesExtract_ALL %>% dplyr::select(
  `ID`,
  `K_ds_ROI_1_L`,
  `K_ds_ROI_2_L`,
  `K_ds_ROI_3_L`,
  `K_ds_ROI_4_L`,
  `K_ds_ROI_5_L`,
  `K_ds_ROI_6_L`,
  `K_ds_ROI_1_R`,
  `K_ds_ROI_2_R`,
  `K_ds_ROI_3_R`,
  `K_ds_ROI_4_R`,
  `K_ds_ROI_5_R`,
  `K_ds_ROI_6_R`
) %>%
  gather(
    `K_ds_ROI_1_L`,
    `K_ds_ROI_2_L`,
    `K_ds_ROI_3_L`,
    `K_ds_ROI_4_L`,
    `K_ds_ROI_5_L`,
    `K_ds_ROI_6_L`,
    `K_ds_ROI_1_R`,
    `K_ds_ROI_2_R`,
    `K_ds_ROI_3_R`,
    `K_ds_ROI_4_R`,
    `K_ds_ROI_5_R`,
    `K_ds_ROI_6_R`,
    key = "ROIS",
    value = "K_ds"
  ) %>%
  separate(
    ROIS,
    into = c("Measure", "Correction", "ROI", "nROI", "hemi"),
    sep = "_"
  ) %>% unite(Measure, Measure, Correction, sep = "_") %>% dplyr::select(`ID`,
                                                                  `nROI`,
                                                                  `hemi`,
                                                                  `K_ds`)

