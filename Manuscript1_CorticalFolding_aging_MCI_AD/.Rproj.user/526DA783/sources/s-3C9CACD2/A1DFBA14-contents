# lambda, b e Ko CTL - com correcao longitudinal ----

decay_AvgThickness_longcor <- filter(dados, Session == "1" , Longitudinal_correction == "yes")  %>%
  group_by(Diagnostic, ROI) %>%
  do(fit_decay_AvgThickness_longcor = rlm(logAvgThickness ~ Age, data = .))

# get the coefficients por intervalo de idade ----
decay_AvgThickness_Coef_longcor = tidy(
  decay_AvgThickness_longcor,
  fit_decay_AvgThickness_longcor,
  conf.int = TRUE,
  conf.level = 0.95
)
decay_AvgThickness_Coef_longcor

decay_AvgThickness_Pred_longcor = augment(decay_AvgThickness_longcor, fit_decay_AvgThickness_longcor)
decay_AvgThickness_Pred_longcor

decay_AvgThickness_R2_longcor = glance(decay_AvgThickness_longcor, fit_decay_AvgThickness_longcor)
decay_AvgThickness_R2_longcor


