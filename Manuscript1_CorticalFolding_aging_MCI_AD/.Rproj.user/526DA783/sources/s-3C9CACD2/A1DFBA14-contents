  decay_AvgThickness_Coef$estimate[decay_AvgThickness_Coef$Diagnostic == "CONTROLE" &
                                     decay_AvgThickness_Coef$term == "Age" &
                                     decay_AvgThickness_Coef$ROI == "P"]

