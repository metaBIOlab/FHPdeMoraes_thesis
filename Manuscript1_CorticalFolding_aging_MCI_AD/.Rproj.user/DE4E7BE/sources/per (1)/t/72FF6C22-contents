# DIVIDE POR TIPO DE MEDIDA ----

# AREA TOTAL

LobesExtract_ALL_TotalArea <- LobesExtract_ALL %>% dplyr::select(
  `ID`,
  `TotalArea_ROI_1_L`,
  `TotalArea_ROI_2_L`,
  `TotalArea_ROI_3_L`,
  `TotalArea_ROI_4_L`,
  `TotalArea_ROI_5_L`,
  `TotalArea_ROI_6_L`,
  `TotalArea_ROI_1_R`,
  `TotalArea_ROI_2_R`,
  `TotalArea_ROI_3_R`,
  `TotalArea_ROI_4_R`,
  `TotalArea_ROI_5_R`,
  `TotalArea_ROI_6_R`
) %>%
  gather(
    `TotalArea_ROI_1_L`,
    `TotalArea_ROI_2_L`,
    `TotalArea_ROI_3_L`,
    `TotalArea_ROI_4_L`,
    `TotalArea_ROI_5_L`,
    `TotalArea_ROI_6_L`,
    `TotalArea_ROI_1_R`,
    `TotalArea_ROI_2_R`,
    `TotalArea_ROI_3_R`,
    `TotalArea_ROI_4_R`,
    `TotalArea_ROI_5_R`,
    `TotalArea_ROI_6_R`,
    key = "ROIS",
    value = "TotalArea"
  ) %>%
  separate(ROIS,
           into = c("Measure", "ROI", "nROI", "hemi"),
           sep = "_") %>% dplyr::select(`ID`,
                                 `nROI`,
                                 `hemi`,
                                 `TotalArea`)


# AvgThickness
