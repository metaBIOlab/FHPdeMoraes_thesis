  `AvgThickness_ROI_3_R`,
  `AvgThickness_ROI_4_R`,
  `AvgThickness_ROI_5_R`,
  `AvgThickness_ROI_6_R`
) %>%
  gather(
    `AvgThickness_ROI_1_L`,
    `AvgThickness_ROI_2_L`,
    `AvgThickness_ROI_3_L`,
    `AvgThickness_ROI_4_L`,
    `AvgThickness_ROI_5_L`,
    `AvgThickness_ROI_6_L`,
    `AvgThickness_ROI_1_R`,
    `AvgThickness_ROI_2_R`,
    `AvgThickness_ROI_3_R`,
    `AvgThickness_ROI_4_R`,
    `AvgThickness_ROI_5_R`,
    `AvgThickness_ROI_6_R`,
